<% layout("layout/boilerPlate.ejs") %> <br /><br />
<div style="height: 75vh; width: 100vw">
  <div class="row flash col-6 offset-3" style="margin-top: 2rem;">
    <div
      class="alert alert-danger mt-3 alert-dismissible fade show"
      role="alert"
    >
      <%= message %>
      <button
        type="button"
        class="btn-close"
        data-bs-dismiss="alert"
        aria-label="Close"
      ></button>
    </div>
  </div>
</div>
